{
  "name": "@devicon/builder",
  "version": "0.0.1",
  "description": "build dev icons packages",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-react": "ts-node build-react.ts && babel tmp/react --out-dir packages/react --copy-files",
    "release": "ts-node publish.ts",
    "build-vue": " ts-node build-vue.ts && babel tmp/vue --out-dir packages/vue --copy-files",
    "build-all": "npm run build-react && npm run build-vue",
    "build-svelte": "ts-node build-svelte.ts && babel tmp/svelte --out-dir packages/svelte --copy-files",
    "publish": "ts-node publish.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-transform-parameters": "^7.18.8",
    "@babel/preset-react": "^7.18.6",
    "@octokit/core": "^4.0.4",
    "@types/degit": "^2.8.3",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/svgo": "^2.6.3",
    "axios": "^0.27.2",
    "babel-preset-minify": "^0.5.2",
    "degit": "^2.8.4",
    "dotenv": "^16.0.1",
    "jsdom": "^20.0.0",
    "svg-to-jsx": "^1.0.4",
    "svgo": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/cli-service": "^5.0.8",
    "react": "^18.2.0",
    "svelte": "^5.14.4",
    "vue-template-compiler": "^2.7.16"
  },
  "keywords": [
    "devicon",
    "builder",
    "react",
    "vue",
    "icons",
    "packages"
  ],
